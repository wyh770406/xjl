<% content_for :title do %>
<h1><%= t 'crud.listing', model: CorrespondingSubject.model_name.human %></h1>
<div class="toolbar">
  <%= link_to t('crud.new'), new_admin_corresponding_subject_path, class: 'btn btn-primary' %>
</div>
<% end %>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
        <th><%= CorrespondingSubject.human_attribute_name(:name) %></th>
        <th><%= CorrespondingSubject.human_attribute_name(:code) %></th>
        <th></th>
    </tr>
  </thead>
  <tbody>
<% @corresponding_subjects.each do |corresponding_subject| %>
    <tr>
      <td><%= corresponding_subject.name %></td>
      <td><%= corresponding_subject.code %></td>
      <td class="control span4">
        <%= link_to t('crud.show'), admin_corresponding_subject_path(corresponding_subject) %>
        <%= link_to t('crud.edit'), edit_admin_corresponding_subject_path(corresponding_subject) %>
        <%= link_to t('crud.destroy'), admin_corresponding_subject_path(corresponding_subject), confirm: t('crud.destroy_confirmation'), method: :delete %>
      </td>
    </tr>
<% end %>
  </tbody>
</table>

<%= paginate @corresponding_subjects %>
